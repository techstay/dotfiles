[core]
	editor = vim
  autocrlf = input
[init]
	defaultBranch = main
[user]
	name = techstay
	email = lovery521@gmail.com
	signingkey = 3E002217712EBA30A53D485F7CDDF9CBDDF9BF2E
[commit]
	gpgsign = true
[rebase]
  autosquash = true
[rerere]
  enabled = true  
[alias]
	l = log --oneline --decorate --all --graph
	co = checkout
	st = status
	d = diff
	ci = commit -m
	rc = rebase --continue
	rs = rebase --skip
	sa = stash apply
	sp = stash pop
	sl = stash list
	ri = rebase -i
	can = commit --amend --no-edit
	br = branch
	unstage = reset HEAD --
	supdate = submodule update --remote --merge
[pull]
	rebase = true
[tag]
	gpgsign = true
	forceSignAnnotated = true
[submodule]
	recurse = true
[help]
	autocorrect = 30
[push]
	recurseSubmodules = on-demand
[url "git@github.com:"]
	insteadOf = gh:
[url "git@github.com:techstay/"]
	insteadOf = my:
